{
  "expo": {
    "name": "Tripture",
    "slug": "Tripture",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tripture-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/tripture.png",
      "resizeMode": "native",
      "backgroundColor": "#CA7FFE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.mobile.tripture"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload an image."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fcde1bbf-8583-4aec-a5de-6fa1d0954e4c"
      }
    }
  }
}
